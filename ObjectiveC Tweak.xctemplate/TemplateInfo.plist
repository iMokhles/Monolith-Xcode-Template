		<dict>
			<key>Group</key>
			<array>
				<string>Hooks</string>
			</array>
			<key>Path</key>
			<string>Hooks/___PACKAGENAMEASIDENTIFIER___.m</string>
		</dict>
		<key>Tweak.h</key>
		<dict>
			<key>Path</key>
			<string>Tweak.h</string>
		</dict>
		<key>Monolith/Monolith.framework</key>
