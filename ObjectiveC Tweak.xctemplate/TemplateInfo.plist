					<string>$(inherited) @executable_path/Frameworks @loader_path/Frameworks</string>
					<key>PRODUCT_NAME</key>
					<string>$(TARGET_NAME)</string>
					<key>SKIP_INSTALL</key>
					<string>YES</string>
